{
  "title": "程序员的知识图谱",
  "author": "webxmsj",
  "plugins": [
    "-lunr",
    "-search",
    "search-pro",
    "-sharing",
    "disqus",
    "github-buttons",
    "editlink",
    "ga",
    "-highlight",
    "prism",
    "prism-themes",
    "bibtex-indexed-cite",
    "anchor-navigation-ex",
    "page-footer-ex",
    "splitter",
    "todo",
    "copy-code-button",
    "expandable-chapters-small",
    "donate"
  ],
  "pluginsConfig": {
    "search-pro": {
      "cutWordLib": "nodejieba",
      "defineWord" : ["Gitbook Use"]
    },
    "anchor-navigation-ex": {
      "showLevel": false
    },
    "disqus": {
      "shortName": "knowledge-graph"
    },
    "github-buttons": {
      "buttons": [{
        "user": "webxmsj",
        "repo": "knowledge-graph",
        "type": "star",
        "size": "small",
        "count": true
      }]
    },
    "editlink": {
      "base": "https://github.com/webxmsj/knowledge-graph/blob/master",
      "label": "Edit",
      "multilingual": false
    },
    "ga": {
      "token": "UA-143340593-1"
    },
    "prism": {
      "ignore": [
        "mermaid",
        "eval-js"
      ],
      "css": [
        "prismjs/themes/prism-solarizedlight.css"
      ]
    },
    "page-footer-ex": {
      "copyright": "By [webxmsj](https://github.com/webxmsj)，使用[知识共享 署名-相同方式共享 4.0协议](https://creativecommons.org/licenses/by-sa/4.0/)发布",
      "markdown": true,
      "update_label": "此页面修订于：",
      "update_format": "YYYY-MM-DD HH:mm:ss"
    },
    "bibtex-indexed-cite": {
      "path": "/"
    },
    "donate": {
      "wechat": "https://imgoss.bfrontend.com/2019-06-28-123045.png",
      "alipay": "https://imgoss.bfrontend.com/2019-06-28-122944.png",
      "title": "",
      "button": "赏",
      "alipayText": "支付宝打赏",
      "wechatText": "微信打赏"
    }
  }
}
